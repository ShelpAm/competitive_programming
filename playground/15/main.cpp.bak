#include <math.h>
#include <stdio.h>

int main()
{
  int n;

  printf("Input n(1<n<=5):\n");
  scanf("%d", &n);

  if (n <= 1 || n > 5) {
    printf("Error!\n");
  }
  else {
    int t = 1;
    t = pow(n + 1, n) + 1;

    for (int i = 0; i != n; ++i) {
      printf("Current num: %d\n", t);
    }

    printf("y=%d\n", t);
  }

  return 0;
}
