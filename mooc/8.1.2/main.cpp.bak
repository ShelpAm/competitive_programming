#include <stdio.h>
int shown_up[10001];
int main()
{
  size_t n;
  scanf("%ull", &n);
  size_t a[n];
  for (size_t i = 0; i != n; ++i) {
    scanf("%ull", &a[i]);
    shown_up[a[i]] = true;
  }
  size_t num = 0;
  for (size_t i = 0; i != n; ++i) {
    for (size_t j = i + 1; j != n; ++j) {
      if (shown_up[a[i] + a[j]]) {
        ++num;
      }
    }
  }
  printf("%ul", num);
  return 0;
}
